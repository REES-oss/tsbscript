-- Scripts:

local function ZLBQJF_fake_script() -- MainFrame.drag 
	local script = Instance.new('LocalScript', MainFrame)

	local function makeDraggable()
		local dragToggle = nil
		local dragSpeed = 0.1
		local dragStart = nil
		local startPos = nil
		local UIS = game:GetService("UserInputService")
	
		local function updateInput(input)
			local delta = input.Position - dragStart
			script.Parent.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		end
	
		script.Parent.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				dragToggle = true
				dragStart = input.Position
				startPos = script.Parent.Position
				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						dragToggle = false
					end
				end)
			end
		end)
	
		script.Parent.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement then
				if dragToggle then
					updateInput(input)
				end
			end
		end)
	end
	
	makeDraggable()
end
coroutine.wrap(ZLBQJF_fake_script)()
local function BFQFS_fake_script() -- CloseAllGui.LocalScript 
	local script = Instance.new('LocalScript', CloseAllGui)

	local UserInputService = game:GetService("UserInputService")
	
	local button = script.Parent
	local gui = button:FindFirstAncestorOfClass("ScreenGui")
	
	button.MouseButton1Click:Connect(function()
		if gui then
			gui.Enabled = false
			print("closed farm ui")
		end
	end)
	
	UserInputService.InputBegan:Connect(function(input, gameProcessed)
		if gameProcessed then return end
	
		if input.KeyCode == Enum.KeyCode.R then
			if gui then
				gui.Enabled = true
				print("farm ui is triggered again")
			end
		end
	end)
	
end
coroutine.wrap(BFQFS_fake_script)()
local function QYKPJAH_fake_script() -- counter.LocalScript 
	local script = Instance.new('LocalScript', counter)

	--[[
	    This is a LocalScript. It should be placed directly inside the TextLabel
	    you want to use to display the session kill count.
	
	    It tracks the number of kills gained *only* during the current game session,
	    not the player's lifetime total stored in leaderstats. The counter resets
	    when the player leaves or the UI is destroyed.
	--]]
	
	-- Services and References
	local Players = game:GetService("Players")
	local TextLabel = script.Parent
	local LocalPlayer = Players.LocalPlayer
	
	-- State and Configuration
	local KILLS_VALUE_NAME = "Kills" -- The name of the value object that holds the overall kill count
	local LEADERSTATS_FOLDER_NAME = "leaderstats"
	local initialKillCount = 0 -- Stores the total kill count when the player joined
	
	-- 1. Check if the LocalPlayer exists (it should, since this is a LocalScript)
	if not LocalPlayer then
		warn("LocalPlayer not found. Script stopping.")
		return
	end
	
	-- 2. Function to update the TextLabel's text
	local function updateKillCountDisplay(currentTotalKills)
		-- Calculate how many kills were gained in *this* session
		local sessionKills = currentTotalKills - initialKillCount
	
		-- Ensure the count is not negative (shouldn't happen, but good practice)
		if sessionKills < 0 then
			sessionKills = 0
		end
	
		-- Format the text. Now it explicitly shows the session count.
		TextLabel.Text = sessionKills .. "0"
	end
	
	-- 3. Main logic to find the Kills value and set up the listener
	local function setupKillTracker()
		-- Wait for the leaderstats folder to be added to the player
		local leaderstats = LocalPlayer:WaitForChild(LEADERSTATS_FOLDER_NAME)
	
		-- Wait for the Kills value object to be inside leaderstats
		local killsValue = leaderstats:WaitForChild(KILLS_VALUE_NAME)
	
		-- *** CRITICAL STEP: Record the starting kill total ***
		initialKillCount = killsValue.Value
	
		-- Handle initial display (this will always show 0 Kills at start)
		updateKillCountDisplay(killsValue.Value)
	
		-- Handle future changes (when the kill count updates)
		killsValue.Changed:Connect(function(newTotalKills)
			updateKillCountDisplay(newTotalKills)
		end)
	
		print("Waiting for" .. LocalPlayer.Name)
		print("Added: " .. initialKillCount)
	end
	
	-- Start the tracking setup
	setupKillTracker()
end
coroutine.wrap(QYKPJAH_fake_script)()
local function PXIY_fake_script() -- FPS.LocalScript 
	local script = Instance.new('LocalScript', FPS)

	local textLabel = script.Parent
	
	local RunService = game:GetService("RunService")
	
	local fps = 0
	local lastUpdate = tick()
	
	RunService.RenderStepped:Connect(function()
		fps = fps + 1
	
		if tick() - lastUpdate >= 1 then
			textLabel.Text = "FPS: " .. fps
			fps = 0
			lastUpdate = tick()
		end
	end)
	
end
coroutine.wrap(PXIY_fake_script)()
local function QSOFQUI_fake_script() -- Ping.LocalScript 
	local script = Instance.new('LocalScript', Ping)

	local textLabel = script.Parent
	local Stats = game:GetService("Stats")
	
	while true do
		local pingStat = Stats.Network.ServerStatsItem["Data Ping"]
		if pingStat then
			local ping = math.floor(pingStat:GetValue()) -- ping in ms
			textLabel.Text = "Ping: " .. ping .. " ms"
		end
	
		task.wait(1) -- update every 1 second
	end
	
end
coroutine.wrap(QSOFQUI_fake_script)()
local function ORAX_fake_script() -- Timer_2.LocalScript 
	local script = Instance.new('LocalScript', Timer_2)

	local textLabel = script.Parent
	
	local seconds = 0
	
	while true do
		seconds += 1
	
		local minutes = math.floor(seconds / 60)
		local secs = seconds % 60
	
		-- Format to 00:00
		local timeString = string.format("%02d:%02d", minutes, secs)
		textLabel.Text = timeString
	
		task.wait(1)
	end
end
coroutine.wrap(ORAX_fake_script)()
local function CWQHFK_fake_script() -- TP.LocalScript 
	local script = Instance.new('LocalScript', TP)

	local Players = game:GetService("Players")
	
	local player = Players.LocalPlayer
	local button = script.Parent
	
	local TARGET_POSITION = Vector3.new(1063, 406, 23006)
	
	button.MouseButton1Click:Connect(function()
		local character = player.Character or player.CharacterAdded:Wait()
		local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
	
		if humanoidRootPart then
			humanoidRootPart.CFrame = CFrame.new(TARGET_POSITION)
			print("tp")
		else
			print("HumanoidRootPart not found")
		end
	end)
	
end
coroutine.wrap(CWQHFK_fake_script)()
