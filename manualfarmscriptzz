local function SQRENKR_fake_script() -- reset.LocalScript 
	local script = Instance.new('LocalScript', reset)

	
	local button = script.Parent
	local Players = game:GetService("Players")
	local RunService = game:GetService("RunService")
	
	local player = Players.LocalPlayer
	
	-- Auto Reset and Stuck System
	local autoResetEnabled = false
	local connection
	local characterAddedConnection
	
	-- Stuck system variables
	local stuckPosition = nil
	local loopConnection = nil
	
	------------------------------------------------
	-- Helper Functions
	------------------------------------------------
	
	-- Freeze player movement
	local function disableMovement()
		local character = player.Character
		if not character then return end
		local humanoid = character:FindFirstChildOfClass("Humanoid")
		if humanoid then
			humanoid.WalkSpeed = 0
			humanoid.JumpPower = 0
		end
	end
	
	-- Restore player movement
	local function enableMovement()
		local character = player.Character
		if not character then return end
		local humanoid = character:FindFirstChildOfClass("Humanoid")
		if humanoid then
			humanoid.WalkSpeed = 16
			humanoid.JumpPower = 50
		end
	end
	
	-- Start Stuck Loop (keeps player at same position)
	local function startStuckLoop()
		if loopConnection then
			loopConnection:Disconnect()
		end
	
		loopConnection = RunService.Heartbeat:Connect(function()
			if player.Character and player.Character:FindFirstChild("HumanoidRootPart") and stuckPosition then
				player.Character:MoveTo(stuckPosition)
			end
		end)
	end
	
	-- Stop the stuck loop
	local function stopStuckLoop()
		if loopConnection then
			loopConnection:Disconnect()
			loopConnection = nil
		end
	end
	
	------------------------------------------------
	-- Main AutoReset + Stuck Toggle
	------------------------------------------------
	local function toggleAutoReset()
		autoResetEnabled = not autoResetEnabled
	
		if autoResetEnabled then
			button.Text = "AutoReset: ON"
	
			-- Save player position when activated
			local char = player.Character
			if char and char:FindFirstChild("HumanoidRootPart") then
				stuckPosition = char.HumanoidRootPart.Position
				startStuckLoop()
			end
	
			-- Function to handle humanoid setup
			local function setupHumanoid(humanoid)
				if connection then
					connection:Disconnect()
				end
	
				connection = humanoid.HealthChanged:Connect(function(health)
					if autoResetEnabled and health < humanoid.MaxHealth then
						-- Instantly kill (auto-reset)
						humanoid.Health = 0
						disableMovement()
					end
				end)
	
				humanoid.Died:Connect(function()
					disableMovement()
				end)
			end
	
			-- Setup current character
			if player.Character then
				local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
				if humanoid then
					setupHumanoid(humanoid)
				end
			end
	
			-- Handle respawn
			if characterAddedConnection then
				characterAddedConnection:Disconnect()
			end
	
			characterAddedConnection = player.CharacterAdded:Connect(function(char)
				local humanoid = char:WaitForChild("Humanoid")
				local root = char:WaitForChild("HumanoidRootPart")
	
				------------------------------------------------
				-- ðŸ§Š NEW SECTION: Freeze on reset, then unfreeze
				------------------------------------------------
				task.wait(0.5)
				stuckPosition = root.Position
				disableMovement()
				startStuckLoop()
	
				-- Auto-unfreeze after a few seconds
				local freezeDuration = 3 -- â³ Change this number if you want longer/shorter freeze
				task.delay(freezeDuration, function()
					stopStuckLoop()
					enableMovement()
				end)
				------------------------------------------------
	
				setupHumanoid(humanoid)
			end)
	
		else
			button.Text = "AutoReset: OFF"
	
			-- Disconnect everything
			if connection then
				connection:Disconnect()
				connection = nil
			end
			if characterAddedConnection then
				characterAddedConnection:Disconnect()
				characterAddedConnection = nil
			end
	
			stuckPosition = nil
			stopStuckLoop()
			enableMovement()
		end
	end
	
	------------------------------------------------
	-- Setup Button
	------------------------------------------------
	button.MouseButton1Click:Connect(toggleAutoReset)
	button.Text = "AutoReset: OFF"
	
	
	
end
coroutine.wrap(SQRENKR_fake_script)()
local function LTKWMIX_fake_script() -- rj.LocalScript 
	local script = Instance.new('LocalScript', rj)

	
	local button = script.Parent
	
	button.MouseButton1Click:Connect(function()
		local TeleportService = game:GetService("TeleportService")
		local Players = game:GetService("Players")
		local player = Players.LocalPlayer
		local placeId = game.PlaceId
	
		TeleportService:Teleport(placeId, player)
	end)
	
end
coroutine.wrap(LTKWMIX_fake_script)()
local function IWPZPT_fake_script() -- freeeeze.LocalScript 
	local script = Instance.new('LocalScript', freeeeze)

	local button = script.Parent
	local Players = game:GetService("Players")
	local player = Players.LocalPlayer
	
	local stuckEnabled = false
	local stuckPosition = nil
	local loopConnection = nil
	
	local function startStuckLoop()
		if loopConnection then
			loopConnection:Disconnect()
		end
	
		loopConnection = game:GetService("RunService").Heartbeat:Connect(function()
			if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
				player.Character:MoveTo(stuckPosition)
			end
		end)
	end
	
	
	local function onCharacterAdded(character)
		if stuckEnabled and stuckPosition then
	
			character:WaitForChild("HumanoidRootPart")
			startStuckLoop()
		end
	end
	
	local function toggleStuck()
		stuckEnabled = not stuckEnabled
	
		if stuckEnabled then
			button.Text = "Freeze Spawn: ON"
	
	
			local char = player.Character
			if char and char:FindFirstChild("HumanoidRootPart") then
				stuckPosition = char.HumanoidRootPart.Position
				startStuckLoop()
			end
	
			player.CharacterAdded:Connect(onCharacterAdded)
		else
			button.Text = "Freeze Spawn: OFF"
			stuckPosition = nil
			if loopConnection then
				loopConnection:Disconnect()
				loopConnection = nil
			end
		end
	end
	
	button.MouseButton1Click:Connect(toggleStuck)
end
coroutine.wrap(IWPZPT_fake_script)()
local function HEGGQON_fake_script() -- AutoClaim.LocalScript 
	local script = Instance.new('LocalScript', AutoClaim)

	local Players = game:GetService("Players")
	local player = Players.LocalPlayer
	local button = script.Parent
	
	local running = false
	
	button.MouseButton1Click:Connect(function()
		if running then
			print("Running emote auto claimer")
			return
		end
	
		running = true
		print("auto claim started")
	
		task.spawn(function()
			while running do
				task.wait(0.1)
	
				local character = player.Character
				if character then
					local comm = character:FindFirstChild("Communicate")
					if comm then
						comm:FireServer({
							Goal = "Emote Spin"
						})
						print("Claimed emote")
					else
						print("communicate not found.. error")
					end
				end
			end
		end)
	end)
	
end
coroutine.wrap(HEGGQON_fake_script)()
local function QAFGB_fake_script() -- antiafk.LocalScript 
	local script = Instance.new('LocalScript', antiafk)

	local Players = game:GetService("Players")
	local VirtualUser = game:GetService("VirtualUser")
	
	local player = Players.LocalPlayer
	local button = script.Parent
	
	local enabled = false
	
	button.MouseButton1Click:Connect(function()
		if enabled then
			print("anti afk is already running")
			return
		end
	
		enabled = true
		print("anti afk triggered")
	
		player.Idled:Connect(function()
			if not enabled then return end
			VirtualUser:CaptureController()
			VirtualUser:ClickButton2(Vector2.new())
			print("fail to kick player.. afk triggered!")
		end)
	end)
	
end
coroutine.wrap(QAFGB_fake_script)()
